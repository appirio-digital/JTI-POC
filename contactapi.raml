- adf-cardinality: |
    {
        "type": "object",
        "properties": {
            "value": {
                "type": "string",
                "enum": [
                    "1 to *",
                    "1 to 1",
                    "* to 1",
                    "* to *"
                ],
                "description": "Cardinality value."
            },
            "sourceAttributes": {
                "type": "string",
                "description": "Attributes in this resource that will be mapped to the destination resource."
            },
            "destinationAttributes": {
                "type": "string",
                "description": "Attributes in the destination resource that will be mapped to this resource."
            }
        },
        "description": "Represents the cardinality between the resources connected by the link.",
        "required": [
            "value",
            "sourceAttributes",
            "destinationAttributes"
        ]
    }
- adf-link-change-indicator: |
