            },
            "limit": {
                "type": "number",
                "defaultValue": 25,
                "minimum": 1,
                "description": "Indicates the limit that was set before obtaining this collection. A collection never returns 'count' bigger than 'limit'."
            },
            "offset": {
                "type": "number",
                "defaultValue": 0,
                "minimum": 0,
