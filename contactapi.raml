            }
        },
        "description": "Additional properties."
    }
- adf-link: |
    {
        "type": "object",
        "properties": {
            "rel": {
                "type": "string",
                "enum": [
                    "self",
                    "lov",
                    "describe",
                    "parent",
                    "child",
                    "enclosure",
                    "predecessor-version",
                    "successor-version",
                    "current",
                    "external"
                ],
                "description": "Represents the relationship type between this resource and the resource which the link is pointing to."
            },
            "href": {
                "type": "string",
                "description": "Either a URI [RFC3986] or a URI Template [RFC6570]."
            },
            "name": {
                "type": "string",
                "description": "Link name, can be used as a unique identifier for the link."
            },
            "kind": {
                "type": "string",
                "enum": [
                    "item",
                    "collection",
                    "describe",
                    "other"
                ],
                "description": "Identifies the kind of resource returned by this link."
            },
            "properties": {
                "$ref": "adf-link-change-indicator"
            },
            "cardinality": {
                "$ref": "adf-cardinality"
            }
        },
        "required": [
            "rel",
            "href",
            "name"
        ]
    }
- adf-links: |
